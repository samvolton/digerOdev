{
  "manifest_version": 3,
  "name": "Focus Gainer",
  "version": "1.0",
  "description": "Gain focus and track your reading progress",
  "icons": {
    "16": "logo16.png",
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo16.png",
      "192": "logo192.png",
      "512": "logo512.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}
